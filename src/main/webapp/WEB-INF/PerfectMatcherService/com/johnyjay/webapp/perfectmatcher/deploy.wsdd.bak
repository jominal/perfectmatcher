<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PerfectMatcherService WSDL service -->

  <service name="PerfectMatcher" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://perfectmatcher.webapp.johnyjay.com"/>
      <parameter name="wsdlServiceElement" value="PerfectMatcherService"/>
      <parameter name="schemaQualified" value="http://perfectmatcher.webapp.johnyjay.com"/>
      <parameter name="wsdlServicePort" value="PerfectMatcher"/>
      <parameter name="className" value="com.matchmaker.webapp.perfectmatcher.PerfectMatcherSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="PerfectMatcher"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="perfectmatcher" qname="operNS:perfectmatcher" xmlns:operNS="http://perfectmatcher.webapp.johnyjay.com" returnQName="retNS:perfectmatcherReturn" xmlns:retNS="http://perfectmatcher.webapp.johnyjay.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="perfectmatcher"/>

  </service>
</deployment>
